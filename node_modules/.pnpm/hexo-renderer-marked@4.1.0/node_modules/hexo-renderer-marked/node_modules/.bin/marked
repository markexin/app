#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/marked@2.1.3/node_modules/marked/bin/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/marked@2.1.3/node_modules/marked/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/marked@2.1.3/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules:/Users/maqun/Documents/fontend-note/node_modules:/Users/maqun/Documents/node_modules:/Users/maqun/node_modules:/Users/node_modules:/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/hexo-renderer-marked@4.1.0/node_modules/hexo-renderer-marked/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/hexo-renderer-marked@4.1.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/marked@2.1.3/node_modules/marked/bin/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/marked@2.1.3/node_modules/marked/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/marked@2.1.3/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules:/Users/maqun/Documents/fontend-note/node_modules:/Users/maqun/Documents/node_modules:/Users/maqun/node_modules:/Users/node_modules:/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/hexo-renderer-marked@4.1.0/node_modules/hexo-renderer-marked/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/hexo-renderer-marked@4.1.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../marked@2.1.3/node_modules/marked/bin/marked" "$@"
else
  exec node  "$basedir/../../../../../marked@2.1.3/node_modules/marked/bin/marked" "$@"
fi
