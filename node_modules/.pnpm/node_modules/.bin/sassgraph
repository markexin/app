#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/sass-graph@2.2.5/node_modules/sass-graph/bin/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/sass-graph@2.2.5/node_modules/sass-graph/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/sass-graph@2.2.5/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules:/Users/maqun/Documents/fontend-note/node_modules:/Users/maqun/Documents/node_modules:/Users/maqun/node_modules:/Users/node_modules:/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/node_modules/sass-graph/bin/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/node_modules/sass-graph/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/sass-graph@2.2.5/node_modules/sass-graph/bin/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/sass-graph@2.2.5/node_modules/sass-graph/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/sass-graph@2.2.5/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules:/Users/maqun/Documents/fontend-note/node_modules:/Users/maqun/Documents/node_modules:/Users/maqun/node_modules:/Users/node_modules:/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/node_modules/sass-graph/bin/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/node_modules/sass-graph/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sass-graph/bin/sassgraph" "$@"
else
  exec node  "$basedir/../sass-graph/bin/sassgraph" "$@"
fi
